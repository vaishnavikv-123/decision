import java.util.*;
public class Vote 
{
public static void main(String[] args) 
{
int age;
Scanner s=new Scanner(System.in);
System.out.println("Enter your age");
age=s.nextInt();
if(age>=18 && age<=20)
System.out.println("Your eligible to vote");
else if(age>20)
System.out.print("Your are a major");
else
System.out.print("Your are not a major");
}
}
